<?xml version="1.0"?>
<robot name="dobot">

<material name="blue">
    <color rgba="0 0 1 1"/>
  </material>
  <material name="green">
    <color rgba="0 1 0 1"/>
  </material>
  <material name="red">
    <color rgba="1 0 0 1"/>
  </material>
  <material name="white">
    <color rgba="1 1 1 1"/>
  </material>

  <link name="base_link" >
    <visual>
       <geometry>
	  <cylinder length="0.12" radius="0.079"/>
       </geometry>
       <material name="blue"/>
    </visual>
  </link>

  <link name="link2" >
    <visual>
       <geometry>
	  <box size="0.147 0.05 0.05"/>
       </geometry>
       <origin rpy="0 0.05 0" xyz="0 0 0"/>
       <material name="red"/>
    </visual>
  </link>

  <link name="link3" >
    <visual>
       <geometry>
	<box size="0.135 0.05 0.05"/>
       </geometry>
       <origin rpy="0 1.57075 0" xyz="0 0 0.247 "/>
       <material name="green"/>
   </visual>
  </link>

 <link name="link4" >
    <visual>
       <geometry>
	 <cylinder length="0.1" radius="0"/>
       </geometry>
       <origin rpy="0 1.57075 0" xyz="0 0.147 0"/>
       <material name="white"/>
   </visual>
  </link>

  <joint name="joint12" type="continuous">
    <parent link="base_link"/>
    <child link="link2"/>
    <origin xyz="0 0 0.06" />
  </joint>

  <joint name="joint23" type="continuous">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0 0 0"/>
  </joint>

  <joint name="joint34" type="continuous">
    <parent link="link3"/>
    <child link="link4"/>
    <origin xyz="0 0.22 0.25"/>
  </joint>

 
</robot>
